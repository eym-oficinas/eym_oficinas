<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>	
        <record id="backup_automatized" forcecreate='True' model="ir.cron">
            <field name="name">Backup Automatized for GITHUB</field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_backup_automatized"/>
            <field name="state">code</field>
            <field name="code">env['backup.automatized'].send_to_github()</field>
        </record>
    </data>
</odoo>
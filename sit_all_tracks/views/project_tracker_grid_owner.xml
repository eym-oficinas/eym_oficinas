<odoo>
  <data>


        <record id="view_project_tracker_grid_tree" model="ir.ui.view">
            <field name="name">view_project_tracker_grid_tree</field>
            <field name="model">sit_all_tracks.sit_all_tracks</field>
            <!-- <field name="type">tree</field> -->
            <!-- <field name="priority" eval="8"/> -->
            <field name="arch" type="xml">
                <!-- <tree string="All Tracks" editable="bottom" create="false" delete="false"> -->
                <tree string="Responsables"  editable="bottom" create="true" >

                    <field name="id" />
                    <!-- <field name="name" /> -->
                    <field name="service_type" invisible="0"/>
                    <field name="responsability" />

       
                </tree>
            </field>
        </record>


        <record model="ir.ui.view" id="view_project_tracker_grid_form">
            <field name="name">sit_all_tracks</field>
            <field name="model">sit_all_tracks.sit_all_tracks</field>
            <field name="arch" type="xml">
                <form string="Responsibl">
                    <sheet>
                        <group>
                          <field name="id" />
                          <!-- <field name="name" /> -->
                          <field name="service_type" invisible="0"/>
                          <field name="responsability" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <!-- action window -->
        <record id="action_cr_projects_tracker_owner" model="ir.actions.act_window">
            <field name="name">Projects Tracker Owner</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">sit_all_tracks.sit_all_tracks</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{"search_default_fieldname":1}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                Click to add a new Projects Tracker
                </p>
                <p>
                Click the Create button to add a new Projects Tracker
                </p>
            </field>
        </record>








        <menuitem sequence='3' id="menu_project_owner" name="Projects responsibles" parent="equino_trading.menu_project_tracker" action='action_cr_projects_tracker_owner' groups="base.group_user"/>
                

                       

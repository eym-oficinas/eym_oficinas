<odoo>
    <template id="view_page_forms_shipping">
        <t t-call="website.layout">
            <div id="wrap">
                <div class="container">
                    <div class="form-shipping-page">
                        <form class="form-form-shipping">

                            <input name="id" id="id" value="1" class="invisible"/>

                            <div class="row">

                                <div class="mb-3 col-4">
                                    <label for="shipper" class="form-label">Shipper</label>
                                    <select class="form-control" name="shipper" id="shipper"/>
                                </div>
                                
                            </div>
                            
                            <div class="row">

                                <div class="mb-3 col-4">
                                    <label for="departure" class="form-label">Departure</label>
                                    <select class="form-control" name="departure" id="departure"/>
                                </div>

                                <div class="mb-3 col-4">
                                    <label for="arrive" class="form-label">Arrive</label>
                                    <select class="form-control" name="arrive" id="arrive"/>
                                </div>

                            </div>

                            <div class="row">

                                <div class="mb-3 col-2">
                                    <label for="entry_date_time" class="form-label">Entry Date </label>
                                    <input type="date-local" class="form-control o_website_form_input o_website_form_date col-1" name="entry_date_time" id="entry_date_time" disabled="1"/>
                                </div>
                                
                                <div class="mb-3 col-2">
                                    <label for="entry_time" class="form-label">Entry Time </label>
                                    <input type="text" class="form-control o_website_form_input  col-1" name="entry_time" id="entry_time" disabled="1"/>
                                </div>
                                
                                <div class="mb-3 col-3" style="display:none">
                                    <label for="stimate_date_shipping" class="form-label">Estimate Date Shipping</label>
                                    <input type="date-local" class="form-control o_website_form_input o_website_form_date col-1" name="stimate_date_shipping" id="stimate_date_shipping" disabled="1"/>
                                </div> 
                                
                                <div class="mb-3 col-5">
                                    <label for="relase_date_time" class="form-label">Release Date and Time</label>
                                    <input type="date-local" class="form-control o_website_form_input o_website_form_date col-1" name="relase_date_time" id="relase_date_time" disabled="1"/>
                                </div> 
                                
                                  
                                
                            </div>
                             
                            <table class="table table-striped table-dark">
                                <thead>
                                    <tr>
                                    <th scope="col" class="col-resgistered_name col-2"><b>Name or Description</b></th>
                                    <th scope="col" class="col-gender col-1"><b>Gender</b></th>
                                    <th scope="col" class="col-specie"><b>Specie</b></th> 
                                    <th scope="col" class="col-breed"><b>Breed</b></th>
                                    <th scope="col" class="col-age" style="width:50px"><b>Age</b></th>
                                    <th scope="col" class="col-color"><b>Color</b></th>
                                    <th scope="col" class="col-microchip_id"><b>Microship</b></th>
                                    <th scope="col" class="col-final_customer col-2"><b>Final Customer</b><small style="font-size: 7px;display: flow-root;">(Name, Address, Phone)</small></th>
                                    <th scope="col" class="col-t_number col-1"><b>T #</b></th>
                                    <th scope="col" class="col-driver_contact col-2"><b>Pick Up Driver <small style="font-size: 7px;display: flow-root;">(Name, Phone)</small></b></th>
                                    <th scope="col" class="col-gear"><b><i class="fa fa-gear"/></b></th>
                                    </tr>
                                </thead>
                                <tbody class="form-items">
                                    <tr class="form-item form-item-1">
                                        <td><input type="text" class="form-control" name="resgistered_name" id="resgistered_name"/></td>
                                        <td><select class="form-control" name="gender" id="gender"/></td>
                                        <td><select class="form-control" name="specie" id="specie"/></td>
                                        <td><select class="form-control" name="breed" id="breed"/></td>
                                        <td><input class="form-control" name="age" id="age"/></td>
                                        <td><select class="form-control" name="color" id="color"/></td>
                                        <td><input class="form-control" name="microchip_id" id="microchip_id"/></td>                         
                                        <td><input class="form-control" name="final_customer" id="final_customer"/></td>
                                        <td><input type="text" class="form-control" name="t_number" id="t_number"/></td>
                                        <td><input class="form-control" name="driver_contact" id="driver_contact"/></td>
                                        <td><div class="btn btn-default form-item-action-remove"><i class="fa fa-trash"/></div></td>
                                    </tr>
                                </tbody>
                            </table>

                            <div class="btn btn-default form-add-item"><i class="fa fa-plus"/> Add </div>
                            <button type="submit" id="form-form-shipping-submit" class="btn btn-primary"><i class="fa fa-cloud"/> Submit</button>

                        </form>
                    </div>
                </div>
            </div>
        </t>
    </template>

    <record id="page_my_wallet" model="website.page">
        <field name="name">Form Shipping</field>
        <field name="website_published">True</field>
        <field name="model">trading.forms</field>
        <field name="url">/forms/shipping</field>
        <field name="view_id" ref="view_page_forms_shipping"/>
    </record>
    
</odoo>